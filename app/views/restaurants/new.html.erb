<h1>Create a new restaurant here!</h1>

<%= simple_form_for [ @restaurant ] do |f| %>
<div class="form-inputs">
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :category %>
  <%= f.input :phone_number %>
</div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
